                        </div>
                        <div class="mobile-navbar-menus-bottom">
                            <div class="mobile-menus">
                                {# <div class="mobile-menu">
                                    <a href="{{ path('home') }}">
                                       {{'Home'|trans}}
                                    </a>
                                </div> #}
                                <div class="mobile-menu">
                                    <a href="{{ path('portfolio') }}">
                                        {{ 'link1'|trans }}
                                    </a>
                                </div>
                                <div class="mobile-menu">
                                    <a href="{{ path('home') }}#testimony">
                                        {{ 'link2'|trans }}
                                    </a>
                                </div>
                                <div class="mobile-menu">
                                    <a href="{{ path('home') }}#process">
                                        {{ 'link3'|trans }}
                                    </a>
                                </div>
                                <div class="mobile-menu">
                                    <a href="{{ path('home') }}#FAQ">
                                        {{ 'link4'|trans }}
                                    </a>
                                </div>
                                {% if (app.request.locale == 'fr') %}
                                <div class="mobile-menu">
									<a href="{{ path('blockchain') }}">
										{{ 'link5'|trans }}
									</a>
								</div>
                                {% endif %}
                                <div class="mobile-menu">
                                    <a href={{ linkQuote }}>
                                        {{'link6'|trans}}
                                    </a>
                                </div>
                                <div class="mobile-menu"></div>
                                <div class="mobile-menu"></div>
                                <div class="mobile-menu">
                                    <a href={{ linkQuote }}>
                                        <button class="pink-btn">{{ 'cta.top'|trans }}</button>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            {% endblock %}

			{% block body %}{% endblock %}

