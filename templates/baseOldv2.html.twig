        <div class="pr__wrapper">
            {% block navbar %}
			<div class="new-navbar navbar-desktop new-navbar--bg-black">
				<div class="box">
					<div class="left">
						<a href="{{ path('home') }}">
							<div class="logo">
								<img src="{{ asset('build/images/newLandingPage/new_logo.png') }}" width="300" height="300"/>
							</div>
						</a>
					</div>
					<div class="mid">
						<div class="mid-menu">
							<a href="{{ path('portfolio') }}">
								{{ 'link1'|trans }}
							</a>
						</div>
						<div class="mid-menu">
							<a href="{{ path('home') }}#testimony">
								{{ 'link2'|trans }}
							</a>
						</div>
						<div class="mid-menu">
							<a href="{{ path('home') }}#process">
								{{ 'link3'|trans }}
							</a>
						</div>
						<div class="mid-menu">
							<a href="{{ path('home') }}#FAQ">
								{{ 'link4'|trans }}
							</a>
						</div>

						{% if (app.request.locale == 'fr' or app.request.locale == 'en') %}
                            <div class="mid-menu">
                                    <a href="{{ path('blockchain') }}">
                                        {{ 'link5'|trans }}
                                    </a>
                            </div>
						{% endif %}
                         <div class="mid-menu">
                            <a href={{ linkQuote }}>
								{{'link6'|trans}}
							</a>
						</div>
					</div>
					<div class="right">
                    {% if app.request.locale == 'hk' %}
                        <a href= {{ linkQuote }} >
                            <button class="white-btn white-btn--text-rose white-btn--bg-transparent">{{ 'cta.top'|trans }}</button>
                        </a>
                    {% elseif app.request.locale == 'ko' %}
                        <a href= {{ linkQuote }} >
                            <button class="white-btn white-btn--text-rose white-btn--bg-transparent">{{ 'cta.top'|trans }}</button>
                        </a>
                    {% else  %}
                        <a href="tel:{{'+33 4 51 20 24 24'|trans}}">
                            <button class="white-btn white-btn--text-rose white-btn--bg-transparent">
                                <img src="{{ asset('build/images/landingPage/tel-icon-4x.png') }}" style="margin-right:10px; width: 10%" alt="icon telephone" width="22" height="22"/>
                                {{'Call us 04 51 20 24 24'|trans}}
                            </button>
                        </a>
                    {% endif %}
					</div>
				</div>
			</div>
            <div class="new-navbar navbar-mobile new-navbar--bg-black">
                <div class="box">
                    <div class="left">
                        <a href="{{ path('home') }}">
                            <div class="logo">
                                <img src="{{ asset('build/images/newLandingPage/new_logo.png') }}" width="300" height="300"/>
                            </div>
