        <div class="pr__wrapper" id="site-wrapper">

            {% block body %}{% endblock %}

            <footer class="pr__footer" id="site-footer">

                <div class="pr__footer__top uk-section uk-section-large">
                    <div class="section-outer">
                        <div class="uk-container">
                            <div class="section-inner">
                                <div class="columns uk-grid" data-uk-grid="">
                                    <div class="pr__cta column">
                                        <div class="inner">
                                            <h2 class="title uk-h1">{{"A project? Let's talk!"|trans}}</h2>
                                        </div>
                                    </div>
                                    <div class="pr__cta column">
                                        <div class="inner">
                                            <a id="pr__contact" href="#pr__contact__form" class="button uk-button uk-button-large uk-button-default"
                                               data-uk-toggle="">{{ 'Ask for a quote'|trans }}</a>
                                        </div>
                                    </div>
                                    <div class="pr__social column">
                                        <div class="inner">
                                            <a href="https://www.linkedin.com/company/kodkodkod-studio" class="icon pr-logo-linkedin" ></a>

                                        </div>
                                    </div>
                                </div>
                            </div><!-- Inner End-->
                        </div><!-- Container End-->
                    </div><!-- Outer End-->
                </div><!-- Footer Top End-->
                <div class="pr__footer__center uk-section uk-section-small">
                    <div class="uk-container">
                        <ul>
                            <li><a href="#">Vietnam<span class="phone">16 Nguyen Hoi, Nguyen An Ninh ward, Vung Tau city, Vietnam</span></a></li>
                            <li><a href="#">France<span class="phone">328 rue du doyen georges chapas 69009 Lyon. </span><div>04 51 20 24 24</div></a></li>

                            <li><a href="#">{{'South Korea'}}<span class="phone">서울특별시 강남구 영동대로 602, 미켈란107 6층 K12<br/>주식회사 호도라(592-84-00057)</span></a></li>

                        </ul>
                    </div>
                </div>
                <div class="pr__footer__bottom uk-section">
                    <div class="section-outer">
                        <div class="uk-container">
                            <div class="section-inner">
                                <div class="columns uk-grid" data-uk-grid="">
                                    <div class="pr__links column">
                                        <div class="inner">
                                            <a href="{{ path('conditions') }}">{{ 'Terms of use'|trans }}</a>
                                            <a href="{{ path('polique') }}">{{'Privacy policies'|trans}}</a>
                                        </div>
                                    </div>
                                    <div class="pr__copyrights column">
                                        <div class="inner">
                                            <p>2021 © <a href="/">KodKodKod</a>, All rights reserved.</p>
                                        </div>
                                    </div>
                                </div>
                            </div><!-- Inner End-->
                        </div><!-- Container End-->
                    </div><!-- Outer End-->
                </div><!-- Footer Bottom End-->

            </footer><!-- Site Footer End-->

        </div><!-- Site Wrapper End -->



        <div id="pr__contact__form" class="pr__contact__form uk-modal-full" data-uk-modal="">
            <div class="uk-modal-dialog" data-uk-height-viewport="">
                <div class="form-outer">
                    <div class="uk-container uk-container-xsmall">
                        <div class="form-inner uk-position-relative">
                            <button class="uk-modal-close-full" type="button" data-uk-close="ratio: 2;">
								<span>{{ 'close'|trans }}</span>
							</button>
                            <h2 class="uk-modal-title uk-h1">{{'A project?'|trans}}</h2>
                            <p>{{'Call us to discuss'|trans}} <a href="tel:{{'+33 4 51 20 24 24'|trans}}"> {{'+33 4 51 20 24 24'|trans}}</a></p>
                            <div class="result"></div>
                            <form class="pr__contact_general pr__form" action="{{ path('send_devis') }}" method="post">
                                <div class="pr__form__group">
                                    <label for="name">{{ 'Your name'|trans }}<span class="required">*</span></label>
                                    <input class="pr-input" id="name" name="name" type="text" required />
                                </div>
                                <div class="pr__form__group">
                                    <label for="email">{{ 'Your email'|trans }}<span class="required">*</span></label>
                                    <input class="pr-input" id="email" name="email" type="text" required />
                                </div>
                                <div class="pr__form__group">
                                    <label for="subject">{{ 'Your project'|trans }}<span class="required">*</span></label>
                                    <input class="pr-input" id="subject" name="subject" type="text" required />
                                </div>
{#                                <div class="pr__form__group">#}
{#                                    <label for="budget">What's your budget range?</label>#}
{#                                    <select class="uk-select" id="budget" name="budget">#}
{#                                        <option>&nbsp;</option>#}
{#                                        <option>Low budget</option>#}
{#                                        <option>$1,000 — $2,000</option>#}
{#                                        <option>$2,000 — $5,000</option>#}
{#                                        <option>$5,000 — $10,000</option>#}
{#                                        <option>$10,000+</option>#}
{#                                    </select>#}
{#                                </div>#}
                                <div class="pr__form__group">
                                    <label for="message">{{ 'Your message'|trans }}<span class="required">*</span></label>
                                    <textarea class="pr-textarea" id="message" name="message" required></textarea>
                                </div>
                                <p class="pr__form__group uk-margin-large">
                                    <button class="uk-button uk-button-large uk-button-primary" type="submit"
											data-sitekey="{{ gg_recaptcha_site_key }}"
											data-callback='onSubmit'
											data-action='submit'
									>{{ 'Validate'|trans }}
									</button>
                                </p>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- Contact Form Popup End -->

