<section class="py-3">
    <div class="container border-bottom py-5">
                <h1 class="display-5 space-grotesk text-center fw-medium pb-5">{{"coaching.clients.title"|trans}}</h1>
        <div class="row justify-content-between align-items-center my-3">
            {% for client in clients %}
                <div class="col-auto">
                    <img src="{{ asset(client.image) }}" alt="{{ client.name }}" width="120" class="img-fluid">
                </div>
            {% endfor %}
        </div>
    </div>
</section>